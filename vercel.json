{
    "version": 2,
    "devCommand": "bun run production",
    "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
    "builds": [
        {
            "src": "/dist/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api",
            "dest": "/dist/index.js"
        }
    ]
}
